<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/lblTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:text="@string/rsTitle"
        android:textColor="@color/design_default_color_error"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/scrollView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="396dp"
        android:layout_height="530dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lblTitle">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <EditText
                android:id="@+id/txtName1stRocket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autofillHints="@string/rsName"
                android:background="@null"
                android:ems="10"
                android:gravity="center"
                android:hint="@string/rsName"
                android:inputType="textPersonName"
                android:singleLine="false"
                android:text="@string/rsName"
                android:textColor="?attr/colorSecondaryVariant"
                android:textSize="24sp"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/txtLaunch1stRocket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autofillHints="@string/rsLaunchDesciption"
                android:background="@null"
                android:gravity="center"
                android:hint="@string/rsLaunchDesciption"
                android:inputType="textMultiLine"
                android:paddingBottom="10dp"
                android:singleLine="false"
                android:text="@string/rsLaunchDesciption"
                android:textSize="18sp" />

            <EditText
                android:id="@+id/txtName2ndRocket"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:autofillHints="@string/rsName"
                android:background="@null"
                android:ems="10"
                android:gravity="center"
                android:hint="@string/rsName"
                android:inputType="textPersonName"
                android:singleLine="false"
                android:text="@string/rsName"
                android:textColor="?attr/colorSecondaryVariant"
                android:textSize="24sp"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/txtLaunch2ndRocket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autofillHints="@string/rsLaunchDesciption"
                android:background="@null"
                android:gravity="center"
                android:hint="@string/rsLaunchDesciption"
                android:inputType="textMultiLine"
                android:paddingBottom="10dp"
                android:singleLine="false"
                android:text="@string/rsLaunchDesciption"
                android:textSize="18sp" />

            <EditText
                android:id="@+id/txtName3rdRocket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autofillHints="@string/rsName"
                android:background="@null"
                android:ems="10"
                android:gravity="center"
                android:hint="@string/rsName"
                android:inputType="textPersonName"
                android:singleLine="false"
                android:text="@string/rsName"
                android:textColor="?attr/colorSecondaryVariant"
                android:textSize="24sp"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/txtLaunch3rdRocket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autofillHints="@string/rsLaunchDesciption"
                android:background="@null"
                android:gravity="center"
                android:hint="@string/rsLaunchDesciption"
                android:inputType="textMultiLine"
                android:paddingBottom="10dp"
                android:singleLine="false"
                android:text="@string/rsLaunchDesciption"
                android:textSize="18sp" />

            <EditText
                android:id="@+id/txtName4thRocket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autofillHints="@string/rsName"
                android:background="@null"
                android:ems="10"
                android:gravity="center"
                android:hint="@string/rsName"
                android:inputType="textPersonName"
                android:singleLine="false"
                android:text="@string/rsName"
                android:textColor="?attr/colorSecondaryVariant"
                android:textSize="24sp"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/txtLaunch4thRocket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autofillHints="@string/rsLaunchDesciption"
                android:background="@null"
                android:gravity="center"
                android:hint="@string/rsLaunchDesciption"
                android:inputType="textMultiLine"
                android:paddingBottom="10dp"
                android:singleLine="false"
                android:text="@string/rsLaunchDesciption"
                android:textSize="18sp" />

            <EditText
                android:id="@+id/txtName5thRocket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autofillHints="@string/rsName"
                android:background="@null"
                android:ems="10"
                android:gravity="center"
                android:hint="@string/rsName"
                android:inputType="textPersonName"
                android:singleLine="false"
                android:text="@string/rsName"
                android:textColor="?attr/colorSecondaryVariant"
                android:textSize="24sp"
                android:textStyle="bold" />

            <EditText
                android:id="@+id/txtLaunch5thRocket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autofillHints="@string/rsLaunchDesciption"
                android:background="@null"
                android:gravity="center"
                android:hint="@string/rsLaunchDesciption"
                android:inputType="textMultiLine"
                android:singleLine="false"
                android:text="@string/rsLaunchDesciption"
                android:textSize="18sp" />
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>